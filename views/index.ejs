<!-- Fast Food -->
<section class="center">
    <h2 class="title" id="hhh">Fast food</h2>
    <div class="accordion" id="accordionExample">
        <div class="card collapseItem">
            <div class="card-header" id="headingOne">
                <h2 class="mb-0">
                    <button class="btn-block text-left foodbtn" type="button" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                        Hamburger
                    </button>
                </h2>
            </div>

            <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordionExample">
                <div class="card-body">

                    <%if(hamburger != '') {%>
                    <%hamburger.forEach(element => {%>
                    <div class="card" style="width: 18rem;">
                        <img src="<%=element.img%>" class="card-img-top" alt="...">
                        <div class="card-body">
                            <hr>
                            <h5 class="card-title"><%=element.title%></h5>
                            <p class="card-text"><%=element.price%> | <%-parseFloat(priceUSD*parseFloat(element.price)).toFixed(2)%>$ | <%-parseFloat(priceKZT*parseFloat(element.price)).toFixed(2)%>₸</p>
                            <a href="#" class="takeOrderbtn">Add to cart</a>
                        </div>
                    </div>
                    <%})%>
                    <%}else{%>
                        <h6>List is empty</h6>
                    <%}%>
                </div>
            </div>
        </div>


        <div class="card collapseItem">
            <div class="card-header" id="headingTwo">
                <h2 class="mb-0">
                    <button class="btn-block text-left collapsed foodbtn" type="button" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                        Pizza
                    </button>
                </h2>
            </div>
            <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionExample">
                <div class="card-body">
                    <%if(pizza != '') {%>
                    <%pizza.forEach(element => {%>
